{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bean-project/FOSUserBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bean-project/IvoryCKEditorBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bean-project/ivory-json-builder"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bean-project/ExcelBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bean-project/gdhh-core"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.52",
        "bean-project/location-bundle": "^0.2",
        "egeloen/ckeditor-bundle": "^6.0.6",
        "friendsofsymfony/user-bundle": "^2.0.5",
        "sonata-project/admin-bundle": "^3.31",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.4",

        "symfony/orm-pack": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",

        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "discard-changes": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C6J9A0VRHW2WD98BX0BZYRJJ",
            "allow-contrib": false
        }
    }
}
